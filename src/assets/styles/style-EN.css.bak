@import url("https://fonts.googleapis.com/css2?family=Poppins:wght@200;300;400;500;600&display=swap");

:root{
  --neutral-50: #F2F3F3;
  --neutral-100: #E4E6E6;
  --neutral-200: #CACDCE;
  --neutral-300: #B0B4B5;
  --neutral-400: #969B9C;
  --neutral-500: #7C8283;
  --neutral-600: #636869;
  --neutral-700: #4A4E4F;
  --neutral-800: #313435;
  --neutral-900: #191A1B;
}
html,
body {
  font-family: "Poppins", sans-serif !important;
  font-size: 16px;
}

@media (min-width: 1900px) {

